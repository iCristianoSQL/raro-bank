# Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/log/*.log
/tmp

# Ignore uploaded files in development.
/storage/*
!/storage/.keep

# Ignore yarn packages.
/yarn-error.log
/yarn-debug.log*
.yarn/

# Ignore Node modules.
/node_modules

# Ignore local environment configuration.
/.env*

# Ignore the default puma PID file.
/tmp/pids/server.pid

# Ignore the default bootsnap cache directory.
/tmp/cache

# Ignore uploaded files in production.
/public/uploads
!/public/uploads/.keep

# Ignore master key for decrypting credentials and more.
/config/master.key

# Ignore encrypted credentials.
/config/credentials.yml.enc

# Ignore any other generated files.
/public/assets
/public/packs
/public/packs-test
/public/packs-test-*

/public/packs-*
/public/webpack

/public/hashed

# Ignore .DS_Store files (macOS).
**/.DS_Store

# Ignore coverage directory generated by SimpleCov.
/coverage/

# Ignore yarn integrity file.
.yarn-integrity

# Ignore Spring application preloader.
/spring/*.pid

# Ignore Webpacker compiled files.
/public/packs
/public/packs-test
/public/assets/.sprockets-manifest-*
/public/assets/.webpack-*
/public/packs-*
/public/hashed

# Ignore system test files.
/tmp/capybara
/tmp/screenshots

# Ignore Redis dump file.
/dump.rdb

# Ignore Byebug command history file.
/.byebug_history

# Ignore the default bootsnap cache directory.
/.bootsnap-cache
